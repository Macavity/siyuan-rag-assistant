  "license": "MIT",
  "homepage": "",
  "repository": "",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "test": "vitest",
    "lint": "eslint . --fix --cache",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "update-version": "node --no-warnings ./scripts/update_version.js",
    "release": "node ./scripts/release.js",
    "release:manual": "node ./scripts/release.js --mode=manual",
    "release:patch": "node ./scripts/release.js --mode=patch",
    "release:minor": "node ./scripts/release.js --mode=minor",
    "release:major": "node ./scripts/release.js --mode=major"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "minimist": "^1.2.8",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.62.1",
    "siyuan": "^1.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.9.0",
    "vite-plugin-static-copy": "^3.0.0",
    "vite-plugin-zip-pack": "^1.0.5",
    "vue": "^3.5.22",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
