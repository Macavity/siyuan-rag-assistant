{
  "name": "siyuan-rag-assistant",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "author": "Alexander Pape",
  "license": "MIT",
  "homepage": "",
  "repository": "",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "test": "vitest",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,vue,json,scss,css}\" \"scripts/**/*.{js,ts}\" \"*.{json,md,js,mjs}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,json,scss,css}\" \"scripts/**/*.{js,ts}\" \"*.{json,md,js,mjs}\"",
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "update-version": "node --no-warnings ./scripts/update_version.js",
    "release": "node ./scripts/release.js",
    "release:manual": "node ./scripts/release.js --mode=manual",
    "release:patch": "node ./scripts/release.js --mode=patch",
    "release:minor": "node ./scripts/release.js --mode=minor",
    "release:major": "node ./scripts/release.js --mode=major",
    "build": "vite build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@types/node": "^20.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.2.0",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "minimist": "^1.2.8",
    "prettier": "^3.6.2",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.62.1",
    "siyuan": "^1.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^6.2.1",
    "vite-plugin-static-copy": "^3.0.0",
    "vite-plugin-zip-pack": "^1.0.5",
    "vue": "^3.5.22"
  },
  "dependencies": {
    "pinia": "^3.0.3"
  }
}
